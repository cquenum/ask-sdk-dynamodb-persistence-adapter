{
  "name": "@ojdoc/ask-sdk-dynamodb-persistence-adapter",
  "version": "1.0.2",
  "description": "DynamoDB based implementation package of PersistenceAdapter interface in ASK SDK v2 for Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run lint",
    "compile": "tsc",
    "test": "cross-env TS_NODE_FILES=true mocha -r ts-node/register \"./tst/**/*.spec.ts\"",
    "lint": "eslint \"lib/**/*.{ts,tsx}\" \"tst/**/*.{ts,tsx}\"",
    "clean": "rm -rf ./dist",
    "reinstall": "rm -rf ./node_modules && npm install",
    "update": "npx npm-check-updates -u && npm install"
  },
  "author": "Ojdoc",
  "contributors": [
    "Codjo Quenum <cquenum@users.noreply.github.com>"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "Alexa",
    "SDK",
    "DynamoDB",
    "Persistence Adapter"
  ],
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.438.0",
    "@aws-sdk/lib-dynamodb": "^3.438.0"
  },
  "peerDependencies": {
    "ask-sdk-core": "^2.12.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.9",
    "@types/mocha": "^10.0.3",
    "@types/node": "^20.8.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "ask-sdk-core": "^2.14.0",
    "ask-sdk-model": "^1.86.0",
    "aws-sdk-client-mock": "^3.0.0",
    "aws-sdk-mock": "^5.8.0",
    "chai": "^4.3.10",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cquenum/ask-sdk-dynamodb-persistence-adapter.git"
  },
  "bugs": {
    "url": "https://github.com/cquenum/ask-sdk-dynamodb-persistence-adapter/issues"
  },
  "homepage": "https://github.com/cquenum/ask-sdk-dynamodb-persistence-adapter#readme",
  "directories": {
    "lib": "lib"
  }
